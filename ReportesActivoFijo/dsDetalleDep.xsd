<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsDetalleDep" targetNamespace="http://tempuri.org/dsDetalleDep.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/dsDetalleDep.xsd" xmlns:mstns="http://tempuri.org/dsDetalleDep.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsDetalleDep" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Reporte">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Empresa" type="xs:string" minOccurs="0" />
							<xs:element name="Titulo" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="Fecha" type="xs:date" minOccurs="0" />
							<xs:element name="CodSucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Sucursal" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Datos">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CodActivo" type="xs:string" minOccurs="0" />
							<xs:element name="NombreActivo" type="xs:string" minOccurs="0" />
							<xs:element name="FechaAdquisicion" type="xs:date" minOccurs="0" />
							<xs:element name="IdDepto" type="xs:string" minOccurs="0" />
							<xs:element name="Departamento" type="xs:string" minOccurs="0" />
							<xs:element name="ValorAdquisicion" type="xs:decimal" minOccurs="0" />
							<xs:element name="Dep_Mensual" type="xs:decimal" minOccurs="0" />
							<xs:element name="Dep_Acum" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoLibros" type="xs:decimal" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>