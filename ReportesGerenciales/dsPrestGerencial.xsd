<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsPrestGerencial" targetNamespace="http://tempuri.org/dsPrestGerencial.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/dsPrestGerencial.xsd"
	xmlns:mstns="http://tempuri.org/dsPrestGerencial.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsPrestGerencial" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="PrestamosGerenciales">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dui" type="xs:string" minOccurs="0" />
							<xs:element name="Nombre" type="xs:string" minOccurs="0" />
							<xs:element name="CodPrestamo" type="xs:string" minOccurs="0" />
							<xs:element name="Monto" type="xs:float" minOccurs="0" />
							<xs:element name="MontoRefinanciamiento" type="xs:float" minOccurs="0" />
							<xs:element name="Tasa_Interes" type="xs:float" minOccurs="0" />
							<xs:element name="FechaOtorgamiento" type="xs:date" minOccurs="0" />
							<xs:element name="FechaVencimiento" type="xs:date" minOccurs="0" />
							<xs:element name="PlazoMeses" type="xs:int" minOccurs="0" />
							<xs:element name="Promocion" type="xs:string" minOccurs="0" />
							<xs:element name="CodEjecMercadeo" type="xs:string" minOccurs="0" />
							<xs:element name="CodEmpleado" type="xs:string" minOccurs="0" />
							<xs:element name="Ejecutivo" type="xs:string" minOccurs="0" />
							<xs:element name="CodTipoCredito" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Reporte">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Fecha" type="xs:date" minOccurs="0" />
							<xs:element name="Empresa" type="xs:string" minOccurs="0" />
							<xs:element name="Titulo" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="CodSucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Sucursal" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>