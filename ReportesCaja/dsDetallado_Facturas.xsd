<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsDetallado_Facturas" targetNamespace="http://tempuri.org/dsDetallado_Facturas.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/dsDetallado_Facturas.xsd"
	xmlns:mstns="http://tempuri.org/dsDetallado_Facturas.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsDetallado_Facturas" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Reporte">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Fecha" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Empresa" type="xs:string" minOccurs="0" />
							<xs:element name="Titulo" type="xs:string" minOccurs="0" />
							<xs:element name="SubTitulo" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="CodSucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Sucursal" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Datos">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="IdMovimiento" type="xs:string" minOccurs="0" />
							<xs:element name="Fecha" type="xs:dateTime" minOccurs="0" />
							<xs:element name="CodItem" type="xs:string" minOccurs="0" />
							<xs:element name="NoCuenta" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="SubTotal_Exento" type="xs:decimal" minOccurs="0" />
							<xs:element name="SubTotal_Gravado" type="xs:decimal" minOccurs="0" />
							<xs:element name="SubTotal_SIva" type="xs:decimal" minOccurs="0" />
							<xs:element name="IdCuentaC" type="xs:string" minOccurs="0" />
							<xs:element name="IdCuentaA" type="xs:string" minOccurs="0" />
							<xs:element name="Tipo" type="xs:string" minOccurs="0" />
							<xs:element name="Cuenta" type="xs:string" minOccurs="0" />
							<xs:element name="Observacion" type="xs:string" minOccurs="0" />
							<xs:element name="Total_Iva" type="xs:decimal" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>