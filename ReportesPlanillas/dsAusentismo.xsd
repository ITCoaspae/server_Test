<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsAusentismo" targetNamespace="http://tempuri.org/dsAusentismo.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/dsAusentismo.xsd" xmlns:mstns="http://tempuri.org/dsAusentismo.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsAusentismo" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Reporte">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Empresa" type="xs:string" minOccurs="0" />
							<xs:element name="Titulo" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="Fecha" type="xs:date" minOccurs="0" />
							<xs:element name="CodSucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Sucursal" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Datos">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CodEmpleado" type="xs:string" minOccurs="0" />
							<xs:element name="Dui" type="xs:string" minOccurs="0" />
							<xs:element name="Nombres" type="xs:string" minOccurs="0" />
							<xs:element name="Apellidos" type="xs:string" minOccurs="0" />
							<xs:element name="FechaInicio" type="xs:dateTime" minOccurs="0" />
							<xs:element name="HoraInicio" type="xs:string" minOccurs="0" />
							<xs:element name="FechaFin" type="xs:dateTime" minOccurs="0" />
							<xs:element name="HoraFin" type="xs:string" minOccurs="0" />
							<xs:element name="Cantidad" type="xs:decimal" minOccurs="0" />
							<xs:element name="Mes" type="xs:int" minOccurs="0" />
							<xs:element name="Observaciones" type="xs:string" minOccurs="0" />
							<xs:element name="CodAusentismo" type="xs:string" minOccurs="0" />
							<xs:element name="MotivoAusentismo" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>