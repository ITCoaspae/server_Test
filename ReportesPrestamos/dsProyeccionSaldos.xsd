<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsProyeccionSaldos" targetNamespace="http://tempuri.org/dsProyeccionSaldos.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/dsProyeccionSaldos.xsd"
	xmlns:mstns="http://tempuri.org/dsProyeccionSaldos.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsProyeccionSaldos" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Reporte">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Fecha" type="xs:date" minOccurs="0" />
							<xs:element name="Empresa" type="xs:string" minOccurs="0" />
							<xs:element name="Titulo" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="CodSucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Sucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Dui" type="xs:string" minOccurs="0" />
							<xs:element name="FechaProyeccion" type="xs:date" minOccurs="0" />
							<xs:element name="NoAsociado" type="xs:string" minOccurs="0" />
							<xs:element name="FechaOtorgado" type="xs:date" minOccurs="0" />
							<xs:element name="Nombre" type="xs:string" minOccurs="0" />
							<xs:element name="FechaVencimiento" type="xs:date" minOccurs="0" />
							<xs:element name="CodPrestamo" type="xs:string" minOccurs="0" />
							<xs:element name="Cuota" type="xs:decimal" minOccurs="0" />
							<xs:element name="TipoCredito" type="xs:string" minOccurs="0" />
							<xs:element name="Categoria" type="xs:string" minOccurs="0" />
							<xs:element name="DiaPago" type="xs:int" minOccurs="0" />
							<xs:element name="SubTotal" type="xs:decimal" minOccurs="0" />
							<xs:element name="Total" type="xs:decimal" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PRMP">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SaldoCapital" type="xs:decimal" minOccurs="0" />
							<xs:element name="InteresPagado" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoDiaInteres" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoCapitalMora" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoDiaInteresMora" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoSeguroDeuda" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoSeguroDanios" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoSeguroVida" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoSeguroDesempleo" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoAportacion" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoAhorro" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoComisionManejo" type="xs:decimal" minOccurs="0" />
							<xs:element name="SaldoOtros" type="xs:decimal" minOccurs="0" />
							<xs:element name="DiasMora" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>