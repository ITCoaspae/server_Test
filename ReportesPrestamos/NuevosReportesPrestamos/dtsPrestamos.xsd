                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT distinct b.NoCuenta,rtrim(c.Nombres)+' '+rtrim(c.Apellido1)+' '+rtrim(c.Apellido2)+' '+rtrim(c.ApellidoCas)[Asociado],
case when b.IdCuenta = '21020101' then 'A la Vista'
when b.IdCuenta ='31010101' then 'Aportaciones' end [TipoCta],
b.Valor
FROM CaTransferencias a 
INNER JOIN CaDet_Transferencias b ON a.CodTransferencia = b.CodTransferencia AND a.Fecha = '20/05/2013'
INNER JOIN Cat_Asociados c ON a.Dui = c.Dui</CommandText>
