<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsAperturaCredito" targetNamespace="http://tempuri.org/dsAperturaCredito.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/dsAperturaCredito.xsd"
	xmlns:mstns="http://tempuri.org/dsAperturaCredito.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsAperturaCredito" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="PRPrestamos">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dui" type="xs:string" />
							<xs:element name="CodPrestamo" type="xs:string" minOccurs="0" />
							<xs:element name="Monto" type="xs:double" minOccurs="0" />
							<xs:element name="Saldo_Capital" type="xs:double" minOccurs="0" />
							<xs:element name="Tasa_Interes" type="xs:double" minOccurs="0" />
							<xs:element name="FechaOtorgamiento" type="xs:dateTime" minOccurs="0" />
							<xs:element name="FechaVencimiento" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Plazo_Meses" type="xs:int" minOccurs="0" />
							<xs:element name="nombres" type="xs:string" minOccurs="0" />
							<xs:element name="nombre_ejecutivo" type="xs:string" minOccurs="0" />
							<xs:element name="CodTipoCredito" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="MontoRefinanciamiento" type="xs:double" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Reporte">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Fecha" type="xs:date" minOccurs="0" />
							<xs:element name="Empresa" type="xs:string" minOccurs="0" />
							<xs:element name="Titulo" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="CodSucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Sucursal" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>