<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsProvisiones_Prestamos" targetNamespace="http://tempuri.org/dsProvisiones_Prestamos.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/dsProvisiones_Prestamos.xsd"
	xmlns:mstns="http://tempuri.org/dsProvisiones_Prestamos.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsProvisiones_Prestamos" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Datos">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Correlativo" type="xs:decimal" minOccurs="0" />
							<xs:element name="CodTipoCredito" type="xs:string" minOccurs="0" />
							<xs:element name="TipoCredito" type="xs:string" minOccurs="0" />
							<xs:element name="CodPrestamo" type="xs:string" minOccurs="0" />
							<xs:element name="NoSocio" type="xs:string" minOccurs="0" />
							<xs:element name="Dui" type="xs:string" minOccurs="0" />
							<xs:element name="Nombres" type="xs:string" minOccurs="0" />
							<xs:element name="Saldo" type="xs:decimal" minOccurs="0" />
							<xs:element name="ValorProvision" type="xs:decimal" minOccurs="0" />
							<xs:element name="FechaProvision" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Estado" type="xs:string" minOccurs="0" />
							<xs:element name="FecUltProv" type="xs:dateTime" minOccurs="0" />
							<xs:element name="TablaAmort" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Reporte">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Fecha" type="xs:date" minOccurs="0" />
							<xs:element name="Empresa" type="xs:string" minOccurs="0" />
							<xs:element name="Titulo" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="CodSucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Sucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Provisionadas" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>