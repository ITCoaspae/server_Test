<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="dsDetallePrestamo" targetNamespace="http://tempuri.org/dsDetallePrestamo.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/dsDetallePrestamo.xsd"
	xmlns:mstns="http://tempuri.org/dsDetallePrestamo.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="dsDetallePrestamo" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Reporte">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Fecha" type="xs:date" minOccurs="0" />
							<xs:element name="Empresa" type="xs:string" minOccurs="0" />
							<xs:element name="Titulo" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="Sucursal" type="xs:string" minOccurs="0" />
							<xs:element name="Firma1" type="xs:string" minOccurs="0" />
							<xs:element name="Firma2" type="xs:string" minOccurs="0" />
							<xs:element name="Firma3" type="xs:string" minOccurs="0" />
							<xs:element name="Firma4" type="xs:string" minOccurs="0" />
							<xs:element name="Cargo1" type="xs:string" minOccurs="0" />
							<xs:element name="Cargo2" type="xs:string" minOccurs="0" />
							<xs:element name="Cargo3" type="xs:string" minOccurs="0" />
							<xs:element name="Cargo4" type="xs:string" minOccurs="0" />
							<xs:element name="Ratificacion" type="xs:string" minOccurs="0" />
							<xs:element name="TipoCredito" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PRDeducciones">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Codigo" type="xs:int" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="Neto" type="xs:decimal" minOccurs="0" />
							<xs:element name="Iva" type="xs:decimal" minOccurs="0" />
							<xs:element name="Total" type="xs:decimal" minOccurs="0" />
							<xs:element name="CodPrestamo" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CAT_ASOCIADOS">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dui" type="xs:string" minOccurs="0" />
							<xs:element name="Apellido1" type="xs:string" minOccurs="0" />
							<xs:element name="Apellido2" type="xs:string" minOccurs="0" />
							<xs:element name="ApellidoCas" type="xs:string" minOccurs="0" />
							<xs:element name="Nombres" type="xs:string" minOccurs="0" />
							<xs:element name="Direccion" type="xs:string" minOccurs="0" />
							<xs:element name="NoSocio" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PRPrestamos">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CodPrestamo" type="xs:string" />
							<xs:element name="DUI" type="xs:string" minOccurs="0" />
							<xs:element name="Cuota" type="xs:double" minOccurs="0" />
							<xs:element name="Monto" type="xs:double" minOccurs="0" />
							<xs:element name="Tasa_Interes" type="xs:double" minOccurs="0" />
							<xs:element name="Tasa_Interes_Mora" type="xs:double" minOccurs="0" />
							<xs:element name="PlazoMeses" type="xs:int" minOccurs="0" />
							<xs:element name="FechaOtorgamiento" type="xs:dateTime" minOccurs="0" />
							<xs:element name="FechaVencimiento" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Saldo_Capital" type="xs:double" minOccurs="0" />
							<xs:element name="CodTipoCredito" type="xs:string" minOccurs="0" />
							<xs:element name="FechaPrimeraCuota" type="xs:dateTime" minOccurs="0" />
							<xs:element name="NumSolicitud" type="xs:int" minOccurs="0" />
							<xs:element name="FechaActa" type="xs:dateTime" minOccurs="0" />
							<xs:element name="NoActa" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PRTotalesPrestamo">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CodPrestamo" type="xs:string" minOccurs="0" />
							<xs:element name="Aprobado" type="xs:date" minOccurs="0" />
							<xs:element name="MontoPrestamo" type="xs:string" minOccurs="0" />
							<xs:element name="TotalDeducciones" type="xs:decimal" minOccurs="0" />
							<xs:element name="Liquido" type="xs:decimal" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CRSolP2Credito">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="NumSolicitud" type="xs:string" minOccurs="0" />
							<xs:element name="CodTipoCredito" type="xs:string" minOccurs="0" />
							<xs:element name="Destino" type="xs:string" minOccurs="0" />
							<xs:element name="FormaPago" type="xs:string" minOccurs="0" />
							<xs:element name="CodEnlace" type="xs:int" minOccurs="0" />
							<xs:element name="CondicionesEspeciales" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CRGarantias">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CodGarantia" type="xs:string" minOccurs="0" />
							<xs:element name="Descripcion" type="xs:string" minOccurs="0" />
							<xs:element name="CodTipoGarantia" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:key name="dsDetallePrestamoKey1">
			<xs:selector xpath=".//mstns:CAT_ASOCIADOS" />
			<xs:field xpath="mstns:Dui" />
		</xs:key>
		<xs:key name="dsDetallePrestamoKey3">
			<xs:selector xpath=".//mstns:PRPrestamos" />
			<xs:field xpath="mstns:CodPrestamo" />
		</xs:key>
		<xs:key name="dsDetallePrestamoKey4">
			<xs:selector xpath=".//mstns:CRGarantias" />
			<xs:field xpath="mstns:CodGarantia" />
		</xs:key>
		<xs:key name="dsDetallePrestamoKey5">
			<xs:selector xpath=".//mstns:CRSolP2Credito" />
			<xs:field xpath="mstns:NumSolicitud" />
			<xs:field xpath="mstns:CodTipoCredito" />
		</xs:key>
	</xs:element>
</xs:schema>